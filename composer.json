{
  "name": "rhayun/snooze",
  "description": "Compabilioty to php 7.1 - made by Thomas Kane - Schedule future notifications and reminders in Laravel",
  "license": "MIT",
  "keywords": [
    "scheduled",
    "notifications",
    "snooze",
    "delayed notifications"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Thomas Kane",
      "email": "thomasjohnkane@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "~5.3 || ~6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.0 || ~4.0.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "rhayun\\Snooze\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rhayun\\Snooze\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "test": "phpunit",
    "test:ci": "phpunit --coverage-clover=coverage.xml"
  },
  "extra": {
    "laravel": {
      "providers": [
        "rhayun\\Snooze\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
